<Window x:Class="Practik.Administrator_Wh"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practik"
        mc:Ignorable="d"
        Title="Administrator_wh" Height="450" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="74*"/>
            <RowDefinition Height="190*"/>
            <RowDefinition Height="161*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="PageFrame" Grid.Row="0" Grid.RowSpan="3" Navigated="PageFrame_Navigated"/>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button FontSize="20" Grid.Column="0" x:Name="Products" Height="55" Click="Products_Click">Товары</Button>
            <Button FontSize="20" Grid.Column="1" x:Name="Orders" Height="55" Click="Orders_Click" >Заказы</Button>
            <Button FontSize="20" Grid.Column="2" x:Name="Reports" Height="55" Click="Reports_Click">Отчеты</Button>
        </Grid>
    </Grid>
</Window>

